{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@omnify/*": [
        "./types/*"
      ],
      "@omnify-base/*": [
        "./node_modules/@omnify-base/*"
      ]
    }
  },
  "include": [
    "omnify.config.ts"
  ]
}