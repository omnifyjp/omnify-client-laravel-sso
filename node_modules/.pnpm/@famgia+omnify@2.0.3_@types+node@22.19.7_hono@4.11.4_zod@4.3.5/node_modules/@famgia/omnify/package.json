{
  "name": "@famgia/omnify",
  "version": "2.0.3",
  "description": "Schema-driven database migration system with TypeScript types and Laravel migrations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "omnify": "./bin/omnify.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "docs",
    "scripts",
    "stubs",
    "ai-guides",
    "README.md"
  ],
  "dependencies": {
    "@famgia/omnify-cli": "2.0.3",
    "@famgia/omnify-core": "2.0.3",
    "@famgia/omnify-types": "2.0.3",
    "@famgia/omnify-typescript": "2.0.3",
    "@famgia/omnify-laravel": "2.0.3",
    "@famgia/omnify-atlas": "2.0.3",
    "@famgia/omnify-mcp": "2.0.3",
    "@famgia/omnify-japan": "2.0.3"
  },
  "keywords": [
    "omnify",
    "schema",
    "migration",
    "laravel",
    "typescript",
    "atlas",
    "database",
    "mcp",
    "ai",
    "claude"
  ],
  "author": "Famgia",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.js"
  }
}