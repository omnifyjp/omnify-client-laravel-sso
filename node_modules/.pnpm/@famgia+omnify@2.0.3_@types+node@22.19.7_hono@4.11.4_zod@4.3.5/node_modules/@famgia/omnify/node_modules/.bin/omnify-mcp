#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/omnify-mcp/dist/index.js" "$@"
else
  exec node  "$basedir/../../../../../../@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/omnify-mcp/dist/index.js" "$@"
fi
