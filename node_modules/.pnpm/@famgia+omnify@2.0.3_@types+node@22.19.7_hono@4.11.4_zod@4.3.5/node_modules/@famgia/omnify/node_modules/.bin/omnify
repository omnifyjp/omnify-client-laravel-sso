#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@famgia+omnify-cli@2.0.3_@types+node@22.19.7_zod@4.3.5/node_modules/@famgia/omnify-cli/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@famgia+omnify-cli@2.0.3_@types+node@22.19.7_zod@4.3.5/node_modules/@famgia/omnify-cli/dist/cli.js" "$@"
fi
