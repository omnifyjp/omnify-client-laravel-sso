#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/omnify-mcp/dist/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/omnify-mcp/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/omnify-mcp/dist/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/omnify-mcp/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules/@famgia/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify-mcp@2.0.3_hono@4.11.4/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@famgia/omnify-mcp/dist/index.js" "$@"
else
  exec node  "$basedir/../@famgia/omnify-mcp/dist/index.js" "$@"
fi
