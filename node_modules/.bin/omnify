#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules/@famgia/omnify/bin/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules/@famgia/omnify/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules/@famgia/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules/@famgia/omnify/bin/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules/@famgia/omnify/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules/@famgia/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/@famgia+omnify@2.0.3_@types+node@22.19.7_hono@4.11.4_zod@4.3.5/node_modules:/Users/f.satoshi/dev/omnify/packages/omnify-client-laravel-sso/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@famgia/omnify/bin/omnify.js" "$@"
else
  exec node  "$basedir/../@famgia/omnify/bin/omnify.js" "$@"
fi
